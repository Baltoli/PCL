external @stdio

fresh FibParamN {
fresh FibParamA {
fresh FibParamB {
fresh FibParamC {
    (
        !(
            in FibParamN(N).
            in FibParamA(A).
            in FibParamB(B).
            in FibParamC(Cont).
            [N = 0] {
                out Cont(A).end
            }.
            out FibParamN(N - 1).
            out FibParamA(A + B).
            out FibParamB(A).
            out FibParamC(Cont)
        )
    |
        in @stdio(X).
        out FibParamN(X).
        out FibParamA(0).
        out FibParamB(1).
        out FibParamC(@stdio)
    )
}}}}
