external @stdio

fresh ExpX {
fresh ExpY {
fresh ExpA {
fresh ExpC {
    (
        !(
            in ExpX(X).
            in ExpY(Y).
            in ExpA(A).
            in ExpC(Cont).
            [Y = 0] {
                out Cont(1).end
            }.
            [Y = 1] {
                out Cont(A * X).end
            }.
            out ExpX(X).
            out ExpY(Y - 1).
            out ExpA(A * X).
            out ExpC(Cont)
        )
    |
        in @stdio(A).
        in @stdio(B).
        out ExpX(A).
        out ExpY(B).
        out ExpA(1).
        out ExpC(@stdio)
    )
}}}}
