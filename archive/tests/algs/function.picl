external @stdio

fresh Identity {
fresh Apply {
fresh Succ {
fresh Zero {
    ((((
        !(
            in Identity(Args).
            in Args(Output).
            in Args(X).
            out Output(X)
        )
    |
        !(
            in Apply(Args).
            in Args(Output).
            in Args(F).
            in Args(X).
            fresh FArgs {
                out F(FArgs).
                out FArgs(Output).
                out FArgs(X)
            }
        )
   )|
        !(
            in Succ(Args).
            in Args(Output).
            in Args(X).
            out Output(X + 1)
        )
   )|
        !(
            in Zero(Args).
            in Args(Output).
            in Args(X).
            out Output(0)
        )
   )|
        fresh ApplyFirst {
        fresh ApplySecond {
            out Apply(ApplyFirst).
            out Apply(ApplySecond).

            out ApplyFirst(@stdio).
            out ApplyFirst(Succ).

            out ApplySecond(ApplyFirst).
            out ApplySecond(Identity).
            out ApplySecond(10)
        }}
    )
}}}}
